{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/next" },
  "defaultProject": "app-1",
  "generators": {
    "@nrwl/react": { "application": { "babel": true } },
    "@nrwl/next": {
      "application": { "style": "styled-components", "linter": "eslint" }
    }
  },
  "projects": {
    "app-1": {
      "root": "apps/app-1",
      "sourceRoot": "apps/app-1",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": { "root": "apps/app-1", "outputPath": "dist/apps/app-1" },
          "configurations": { "production": {} }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": { "buildTarget": "app-1:build", "dev": true },
          "configurations": {
            "production": {
              "buildTarget": "app-1:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": { "buildTarget": "app-1:build:production" }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/app-1"],
          "options": {
            "jestConfig": "apps/app-1/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/app-1/**/*.{ts,tsx,js,jsx}"] }
        }
      }
    },
    "app-1-e2e": {
      "root": "apps/app-1-e2e",
      "sourceRoot": "apps/app-1-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/app-1-e2e/cypress.json",
            "tsConfig": "apps/app-1-e2e/tsconfig.e2e.json",
            "devServerTarget": "app-1:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "app-1:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/app-1-e2e/**/*.{js,ts}"] }
        }
      }
    },
    "app-2": {
      "root": "apps/app-2",
      "sourceRoot": "apps/app-2",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": { "root": "apps/app-2", "outputPath": "dist/apps/app-2" },
          "configurations": { "production": {} }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": { "buildTarget": "app-2:build", "dev": true },
          "configurations": {
            "production": {
              "buildTarget": "app-2:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": { "buildTarget": "app-2:build:production" }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/app-2"],
          "options": {
            "jestConfig": "apps/app-2/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/app-2/**/*.{ts,tsx,js,jsx}"] }
        }
      }
    },
    "app-2-e2e": {
      "root": "apps/app-2-e2e",
      "sourceRoot": "apps/app-2-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/app-2-e2e/cypress.json",
            "tsConfig": "apps/app-2-e2e/tsconfig.e2e.json",
            "devServerTarget": "app-2:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "app-2:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/app-2-e2e/**/*.{js,ts}"] }
        }
      }
    },
    "shared-components": {
      "root": "libs/shared-components",
      "sourceRoot": "libs/shared-components/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/shared-components",
            "tsConfig": "libs/shared-components/tsconfig.lib.json",
            "project": "libs/shared-components/package.json",
            "entryFile": "libs/shared-components/src/index.ts",
            "external": ["react", "react-dom", "react-is", "styled-components"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [{ "glob": "README.md", "input": ".", "output": "." }]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared-components/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared-components"],
          "options": {
            "jestConfig": "libs/shared-components/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "app1-helpers": {
      "root": "libs/app1-helpers",
      "sourceRoot": "libs/app1-helpers/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/app1-helpers/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/app1-helpers"],
          "options": {
            "jestConfig": "libs/app1-helpers/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "app2-helpers": {
      "root": "libs/app2-helpers",
      "sourceRoot": "libs/app2-helpers/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/app2-helpers/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/app2-helpers"],
          "options": {
            "jestConfig": "libs/app2-helpers/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
